srcdir =	@srcdir@
VPATH =		@srcdir@
top_srcdir =	@top_srcdir@

VERSION=@BIND9_VERSION@

@BIND9_MAKE_INCLUDES@

CINCLUDES =
CDEFINES =	@USE_GSSAPI@
CWARNINGS =

LIBS =		@LIBS@

TARGETS =	gssapi_krb@EXEEXT@

SRCS =		gssapi_krb.c

GSSAPIKRBOBJS      = gssapi_krb.@O@

OBJS =          ${GSSAPIKRBOBJS}

@BIND9_MAKE_RULES@

CFLAGS =	@CFLAGS@ @SO_CFLAGS@

gssapi_krb@EXEEXT@: ${GSSAPIKRBOBJS}
	${LIBTOOL_MODE_LINK} ${PURIFY} ${CC} ${CFLAGS} ${LDFLAGS} \
		-o $@ ${GSSAPIKRBOBJS} ${LIBS}

clean distclean::
	rm -f ${TARGETS}
